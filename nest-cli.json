{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "rabbitmq-config": {
      "type": "library",
      "root": "libs/rabbitmq-config",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq-config/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "profiles": {
      "type": "application",
      "root": "apps/profiles",
      "entryFile": "main",
      "sourceRoot": "apps/profiles/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profiles/tsconfig.app.json"
      }
    },
    "postgres-typeorm": {
      "type": "library",
      "root": "libs/postgres-typeorm",
      "entryFile": "index",
      "sourceRoot": "libs/postgres-typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgres-typeorm/tsconfig.lib.json"
      }
    }
  }
}